<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="EvaluacionP2TLopez.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:EvaluacionP2TLopez.ViewsTomasLopez"
    Shell.FlyoutBehavior="Disabled"
    Title="EvaluacionP2TLopez">

    <TabBar>
        <Tab Title="Usuario" Icon="telefono.png">
            <ShellContent
                Title="Tlopez_Usuario"
                ContentTemplate="{DataTemplate views:TLopezUsuarioView}"
                Route="ViewsTomasLopez.TLopezUsuarioView"/>
        </Tab>
        <Tab Title="Apellidos" Icon="home.png">
            <ShellContent
                Title="Tlopez_Nombres"
                ContentTemplate="{DataTemplate views:GridTlopez}"
                Route="ViewsTomasLopez.GridTlopez"/>
        </Tab>

    </TabBar>

</Shell>
